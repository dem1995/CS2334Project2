package mckn3023Project2;

/**
 *Holds a PersonList of people that contains Person objects, emulating a city's population. 
 */
public class City implements Comparable<City> {
	
	//Instance variables~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	/**
	 * Holds all of the people in this city
	 */
	private PersonList personList;
	
	//Instance method
	/**
	 * Creates a city and an empty PersonList
	 */
	public City()
	{
		this.personList=new PersonList();
	}
	
	//Getter methods
	/**
	 * Gets the PersonList personList
	 * @return A PersonList of Person objects in this City's PersonList personList
	 */
	public PersonList getPersonList()
	{
		return personList;
	}
	
	//Interface-required methods
	public int compareTo(City city) {
		// TODO Auto-generated method stub
		return 0;
	}
	
	
}
